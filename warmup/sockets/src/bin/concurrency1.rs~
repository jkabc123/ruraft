use std::sync::mpsc::channel;
use std::{thread, time};

fn main() -> std::io::Result<()> {
    let t1 = thread::spawn(|| {
        countdown(10);
    });
    let t2 = thread::spawn(|| {
        countup(5);
    });
    println!("Waiting");
    t1.join().unwrap();
    t2.join().unwrap();
    println!("Goodbye");
    Ok(())
}
